{
  "name": "saferide-connect",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web --output-dir web-build",
    "build:web:dev": "expo start --web --no-dev --minify",
    "predeploy": "expo export --platform web --output-dir web-build || echo 'Build failed, using existing dist'",
    "deploy": "npm run predeploy && firebase deploy --only hosting",
    "deploy:full": "npm run predeploy && firebase deploy",
    "deploy:force": "firebase deploy --only hosting"
  },
  "dependencies": {
